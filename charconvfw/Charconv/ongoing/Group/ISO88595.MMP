/**
@file

@SYMPurpose iso88595.cpl ISO-8859-5 converter
*/

#include <platform_paths.hrh>

target			ISO88595.DLL
targettype		PLUGIN

CAPABILITY All -Tcb

uid				0x10009D8D 0x102044EF
VENDORID 0x70000001

sourcepath		../Source/foreign/plugins
source			ISO88595.CPP

sourcepath		../Source/generatedcpp
source			g_iso88595.cpp

// This is a SYSTEMINCLUDE macro containing the middleware
// layer specific include directories
MW_LAYER_SYSTEMINCLUDE
OS_LAYER_SYSTEMINCLUDE

systeminclude	../Include
SYSTEMINCLUDE   ../../../../inc
SYSTEMINCLUDE   ../../../inc

library			EUSER.LIB CHARCONV.LIB ECOM.LIB	

sourcepath		../Source/foreign/plugins
start resource		ISO88595.RSS
targetpath		/resource/charconv
lang			sc
end

sourcepath		../Source/foreign/plugins
start resource		102044EF.rss
#ifdef SYMBIAN_SECURE_ECOM
target			ISO88595.RSC
#endif
end